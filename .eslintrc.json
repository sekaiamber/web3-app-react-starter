{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": [
    "html",
    "react",
    "jsx-a11y",
    "import"
  ],
  "globals": {
    "__VERSION__": false,
    "__CONFIG__": false
  },
  "rules": {
    "prefer-destructuring": 0,
    "import/no-unresolved": 0,
    "no-new": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "prefer-template": 0,
    "max-len": 0,
    "no-debugger": 2,
    "react/wrap-multilines": 0,
    "import/extensions": 0,
    "react/prop-types": 0,
    "new-cap": 0,
    "react/jsx-no-bind": 0,
    "linebreak-style": 0,
    "import/no-named-as-default": 0,
    "class-methods-use-this": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "react/no-string-refs": 0,
    "react/jsx-boolean-value": 0,
    "react/prefer-stateless-function": 0,
    "react/no-multi-comp": 0,
    "react/jsx-filename-extension": 0,
    "no-extend-native": 0,
    "no-return-assign": 0,
    "no-mixed-operators": 0,
    "object-curly-newline": 0,
    "jsx-a11y/no-noninteractive-element-interactions": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "eslint-disable-next-line": 0,
    "jsx-a11y/alt-text": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/button-has-type": 0,
    "react/sort-comp": [1, {
      "order": [
        "static-variables",
        "static-methods",
        "instance-variables",
        "lifecycle",
        "everything-else",
        "render",
        "renderHeaderLeftIcon",
        "renderHeaderRightIcon",
        "renderHeaderExtraContent"
      ],
      "groups": {
        "lifecycle": [
          "displayName",
          "propTypes",
          "contextTypes",
          "childContextTypes",
          "mixins",
          "statics",
          "defaultProps",
          "constructor",
          "getDefaultProps",
          "state",
          "getInitialState",
          "getChildContext",
          "getDerivedStateFromProps",
          "componentPageInitMatch",
          "componentPageInitScroll",
          "componentPageRecover",
          "componentWillMount",
          "UNSAFE_componentWillMount",
          "componentDidMount",
          "componentWillReceiveProps",
          "UNSAFE_componentWillReceiveProps",
          "shouldComponentUpdate",
          "componentWillUpdate",
          "UNSAFE_componentWillUpdate",
          "getSnapshotBeforeUpdate",
          "componentDidUpdate",
          "componentPageDidChange",
          "componentDidCatch",
          "componentWillUnmount"
        ]
      }
    }]
  }
}

